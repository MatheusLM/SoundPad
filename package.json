{
	"name": "electron-quick-start-typescript",
	"version": "1.0.0",
	"description": "A SoundPad Application created with TypeScript and Electron",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"lint": "eslint -c .eslintrc --ext .ts ./src",
		"start": "electron ./dist/main.js",
		"dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"tsc && npm run start\""
	},
	"author": "Matheus Lopes Marques",
	"github": "https://github.com/MatheusLM/SoundPad",
	"license": "CC0-1.0",
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.207",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"electron": "^19.0.7",
		"eslint": "^7.32.0",
		"nodemon": "^2.0.18",
		"ts-node": "^10.8.1",
		"typescript": "^4.7.2"
	}
}
